import Sound.Tidal.OscStream

sd ip port = do s <- makeConnection "127.0.0.1" port superDirtSlang
                let backend = Backend s (\_ _ _ -> return ())
                st <- Sound.Tidal.Stream.state backend dirt
                return $ setter st

